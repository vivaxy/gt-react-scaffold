{
  "name": "react-scaffold",
  "version": "1.0.1",
  "description": "react-scaffold",
  "main": "webpack.config.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --progress",
    "beta": "NODE_ENV=beta npm run build",
    "production": "NODE_ENV=production npm run build",
    "build": "npm run build:clean && npm run build:webpack",
    "build:clean": "rm -rf ./release && mkdir ./release",
    "build:webpack": "webpack --progress --display-chunks --display-reasons --display-error-details --display-modules --display-exclude > ./release/build.log"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vivaxy/react-scaffold.git"
  },
  "keywords": [
    "react",
    "react-scaffold"
  ],
  "author": "vivaxy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vivaxy/react-scaffold/issues"
  },
  "homepage": "https://github.com/vivaxy/react-scaffold#readme",
  "devDependencies": {
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.24.0",
    "file-loader": "^0.9.0",
    "glob": "^7.1.1",
    "graceful-fs": "^4.1.9",
    "html-webpack-plugin": "^2.22.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "material-ui": "^0.15.4",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.2",
    "react-dom": "^15.3.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-pianist": "^0.0.9",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-tap-event-plugin": "^1.0.0",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "tiny-cookie": "^0.5.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-visualizer-plugin": "^0.1.5"
  },
  "dependencies": {}
}
